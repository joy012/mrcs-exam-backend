      "AuthCompleteProfileDto": {
        "type": "object",
        "properties": {
          "firstName": {
            "type": "string",
            "minLength": 1,
            "description": "First name"
          },
          "lastName": {
            "type": "string",
            "minLength": 1,
            "description": "Last name"
